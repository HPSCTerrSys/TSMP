      FORTRANNOUNDERSCORE)
         THIS_FUNCTION=testflib;;

   esac

   # Checks if the LIBRARY from the argument list can be used to link
   # a C test program with testflib
   as_ac_Lib=`$as_echo "ac_cv_lib_$casc_lib_name''_$THIS_FUNCTION" | $as_tr_sh`
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $THIS_FUNCTION in -l$casc_lib_name" >&5
