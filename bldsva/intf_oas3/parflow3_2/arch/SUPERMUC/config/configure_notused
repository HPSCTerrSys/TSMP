fi







  flag=`echo "-Wconversion" | $SED 'y% .=/+-(){}<>:*,%_______________%'`

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the C compiler accepts the -Wconversion flag" >&5
$as_echo_n "checking whether the C compiler accepts the -Wconversion flag... " >&6; }
